<?xml version="1.0" encoding="utf-8"?>
<animated-selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <bitmap android:src="@drawable/textinput_qwerty_emoticon_ic_crown" android:tint="@color/drawable_color_emoticon_category_selected" />
    </item>
    <item android:state_pressed="true">
        <bitmap android:src="@drawable/textinput_qwerty_emoticon_ic_crown" android:tint="@color/drawable_color_emoticon_category_pressed" />
    </item>
    <item>
        <bitmap android:src="@drawable/textinput_qwerty_emoticon_ic_crown" android:tint="@color/drawable_color_emoticon_category_normal" />
    </item>
</animated-selector>