<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:settings="http://schemas.android.com/apk/res/com.android.settings"
  xmlns:touchwiz="http://schemas.android.com/apk/res/touchwiz">
    <com.android.settings.UnclickablePreference android:title="@string/accessibility_notification_summary" android:key="notification_reminder_guide" />
    <PreferenceCategory android:title="@string/notification_app_settings_button" android:key="notification_reminder_category_options">
        <SwitchPreference android:title="@string/notification_reminder_preference_vibrate" android:key="notification_reminder_vibrate" />
        <SwitchPreference android:title="@string/notification_reminder_preference_led_title" android:key="notification_reminder_indicator" android:summary="@string/notification_reminder_preference_led_summary" />
        <com.android.settings.DropDownPreference android:title="@string/accessibility_notification_reminder_time" android:key="notification_time_interval" android:widgetLayout="@layout/round_more_icon" />
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/notification_reminder_category_title_packages" android:key="notification_reminder_category_packages" />
</PreferenceScreen>